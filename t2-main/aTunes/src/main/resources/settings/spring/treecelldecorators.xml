<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"

	default-autowire="byName" default-lazy-init="true">

	<bean id="albumTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.AlbumTreeCellDecorator" />
	<bean id="artistTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.ArtistTreeCellDecorator" />
	<bean id="folderTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.FolderTreeCellDecorator" />
	<bean id="genreTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.GenreTreeCellDecorator" />
	<bean id="incompleteTagsTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.IncompleteTagsTreeCellDecorator" />
	<bean id="podcastFeedTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.PodcastFeedTreeCellDecorator" />
	<bean id="radioTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.RadioTreeCellDecorator" />
	<bean id="stringTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.StringTreeCellDecorator" />
	<bean id="tooltipTreeCellDecorator"
		class="net.sourceforge.atunes.kernel.modules.navigator.TooltipTreeCellDecorator" />
	<bean id="unknownElementTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.UnknownElementTreeCellDecorator" />
	<bean id="yearTreeCellDecorator"
		class="net.sourceforge.atunes.gui.views.decorators.YearTreeCellDecorator" />
	<bean id="navigationTreeRootCellDecorator" class="net.sourceforge.atunes.gui.views.decorators.NavigationTreeRootTreeCellDecorator"/>

</beans>