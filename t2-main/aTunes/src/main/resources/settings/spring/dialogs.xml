	</bean>
	
	<bean id="recommendedEventsDialog" class="net.sourceforge.atunes.gui.views.dialogs.RecommendedEventsDialog" scope="prototype">
		<constructor-arg ref="frame"/>
		<constructor-arg ref="controlsBuilder"/>
